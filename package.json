{
  "name": "shopify-checkout-step-manager",
  "author": "Rogelio Negrete (Weffe)",
  "sideEffects": false,
  "version": "3.0.0",
  "main": "dist/index.js",
  "module": "dist/checkoutstepmanager.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "shopify",
    "shopify checkout"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Weffe/shopify-checkout-step-manager"
  },
  "bugs": {
    "url": "https://github.com/Weffe/shopify-checkout-step-manager/issues"
  },
  "homepage": "https://github.com/Weffe/shopify-checkout-step-manager",
  "scripts": {
    "start": "tsdx watch --name CheckoutStepManager --format cjs,esm,umd",
    "build": "tsdx build --name CheckoutStepManager --format cjs,esm,umd",
    "test": "tsdx test",
    "lint": "tsdx lint --fix",
    "prettier": "prettier --write 'src/**/*.ts'",
    "prerelease": "run-s test build",
    "release": "standard-version"
  },
  "prettier": {
    "printWidth": 130,
    "trailingComma": "es5",
    "tabWidth": 4,
    "useTabs": false,
    "singleQuote": true,
    "arrowParens": "always",
    "endOfLine": "lf"
  },
  "peerDependencies": {
    "jquery": ">=2.2.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@types/jest": "^24.0.19",
    "@types/jquery": "^2.0.54",
    "jquery": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "standard-version": "^7.0.0",
    "tsdx": "^0.11.0",
    "tslib": "^1.10.0",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "tiny-invariant": "^1.0.6",
    "tiny-warning": "^1.0.3"
  },
  "jest": {
    "moduleNameMapper": {
      "^~/(.*)$": "<rootDir>/src/$1"
    }
  }
}
